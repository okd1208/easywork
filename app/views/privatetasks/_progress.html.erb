<% if p_task.progress != "2020-01-01 00:00:00" %>
    <%= link_to "完了済", "/privatetasks/#{p_task.id}/unprogres", method: "post", data: { confirm: "未完了に戻しますか？" },class: 'btn btn-primary btn-sm'%>
<% else %>
    <%= link_to "未完了", "/privatetasks/#{p_task.id}/progres", method: "post", class: 'btn btn-danger btn-sm'%>
<% end %>